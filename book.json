{
    "plugins": [
        "youtube",
        "disqus",
        "edit-link",
        "prism",
        "-highlight",
        "github",
        "anchorjs",
        "rss"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "bit-framework"
        },
        "edit-link": {
            "base": "https://github.com/bit-foundation/bit-framework/tree/master",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/bit-foundation/bit-framework/"
        },
        "theme-default": {
            "styles": {
                "website": "docs/styles/main.css"
            }
        },
        "rss": {
            "title": "Bit Framework docs",
            "description": "Rss for docs located in https://docs.bit-framework.com/",
            "author": "Bit Foundation",
            "site_url": "https://docs.bit-framework.com/",
            "feed_url": "https://docs.bit-framework.com/rss",
            "managingEditor": "ysmoradi@outlook.com (Yaser Moradi)",
            "webMaster": "ysmoradi@outlook.com (Yaser Moradi)",
            "categories": [
                "C#",
                "TypeScript",
                "JavaScript"
            ]
        }
    }
}
