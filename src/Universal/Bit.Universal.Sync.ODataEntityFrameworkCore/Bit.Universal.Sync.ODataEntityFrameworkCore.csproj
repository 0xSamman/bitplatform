    <PackageTags>$(PackageTags) Sync</PackageTags>
    <RootNamespace>Bit.Sync.ODataEntityFrameworkCore</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bit.System.Linq.Dynamic" Version="1.1.0" />
