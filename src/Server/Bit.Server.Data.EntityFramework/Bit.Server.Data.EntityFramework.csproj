<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="../../Bit.Build.props" />

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageTags>$(PackageTags) Entity-Framework EF Database Microsoft Data O/RM ADO.NET</PackageTags>
    <RootNamespace>Bit.Data.EntityFramework</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bit.Server.Data\Bit.Server.Data.csproj" />
    <PackageReference Include="EntityFramework" Version="6.4.4" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.3.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>