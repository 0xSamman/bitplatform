<Project Sdk="MSBuild.Sdk.Extras/2.0.54">

  <Import Project="../CSharpClient.Build.props" />

  <PropertyGroup>
    <TargetFrameworks>MonoAndroid90;Xamarin.iOS10;uap10.0.16299;netstandard2.0;netcoreapp2.0;</TargetFrameworks>
    <PackageTags>$(PackageTags) OData</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.7">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Simple.OData.V4.Client" Version="5.10.0.1" />
    <PackageReference Include="Microsoft.OData.Core" Version="7.6.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Bit.CSharpClient.Rest\Bit.CSharpClient.Rest.csproj" />
  </ItemGroup>

</Project>
