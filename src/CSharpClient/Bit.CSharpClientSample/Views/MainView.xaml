<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Bit.CSharpClientSample.Views.MainView" Title="Main">
    <StackLayout>
        <Button Text="Send http request" Command="{Binding SendHttpRequest}" />
        <Button Text="Send OData request" Command="{Binding SendODataRequest}" />
        <Button Text="Logout" Command="{Binding Logout}" />
    </StackLayout>
</ContentPage>