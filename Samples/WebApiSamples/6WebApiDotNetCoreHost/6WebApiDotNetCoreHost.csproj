<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <AssemblyName>WebApiDotNetCoreHost</AssemblyName>
    <RootNamespace>WebApiDotNetCoreHost</RootNamespace>
    <PackageConflictPreferredPackages>Microsoft.Private.CoreFx.NETCoreApp;runtime.win-x64.Microsoft.Private.CoreFx.NETCoreApp;$(PackageConflictPreferredPackages)</PackageConflictPreferredPackages>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Private.CoreFx.NETCoreApp" Version="4.5.0-preview3-26513-0" />
    <PackageReference Include="Bit.OwinCore" Version="1.3.90" />
    <PackageReference Include="Bit.OwinCore.AspNetCore21Upgrade" Version="1.3.90" />
    <PackageReference Include="Bit.WebApi" Version="1.3.90" />
    <PackageReference Include="BitCodeAnalyzer" Version="1.3.90" />
  </ItemGroup>
  <ItemGroup>
    <Content Update="environments.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>
