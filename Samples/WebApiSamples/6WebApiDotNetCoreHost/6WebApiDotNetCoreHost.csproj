<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>WebApiDotNetCoreHost</AssemblyName>
    <RootNamespace>WebApiDotNetCoreHost</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Bit.OwinCore" Version="0.9.7" />
    <PackageReference Include="Bit.OwinCore.AspNetCore2Upgrade" Version="0.9.7" />
    <PackageReference Include="Bit.WebApi" Version="0.9.7" />
    <PackageReference Include="BitCodeAnalyzer" Version="1.2.6.2" />
    <PackageReference Include="Microsoft.AspNetCore.All" Version="2.0.6" />
  </ItemGroup>
  <ItemGroup>
    <Content Update="environments.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
</Project>